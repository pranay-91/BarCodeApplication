@using System.Web.Optimization


<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title - Barcode Application</title>
    @Styles.Render("~/styles");
</head>

<body onload="startTime()">
    <div class="container body-content">
        <div id="watch">
            <div id="clock"></div>
            <div id="date"></div>
        </div>
        <div class="jumbotron">
            <p class="clear:both">
                <h1>Traveller</h1>
            </p>
        </div>
        @RenderBody()
    </div>
    
    @Scripts.Render("~/scripts")
    @RenderSection("Scripts", false)

</body>
</html>
